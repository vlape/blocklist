{
  "name": "iTerm Traffic- Ping",
  "description": "Allow iTerm outbound traffic",
  "rules": [
    {
      "action": "allow",
      "direction": "outgoing",
      "priority": "regular",
      "process": "\/Applications\/iTerm.app",
      "via": /sbin/ping",
      "owner" : "me",
      "ports" : "any",
      "protocol": "any",
      "destination": "any"
    }
  ]
}
